    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>±</m:mo><m:msqrt>
      <m:mn>6</m:mn>
     </m:msqrt>
     
    </m:mrow>
    <m:mn>2</m:mn>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id11026964"><problem id="fs-id11045199"><para id="fs-id11045200"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>    <m:mi>x</m:mi>    <m:mn>2</m:mn>   </m:msup>   <m:mo>−</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id11044635"><problem id="fs-id11044636"><para id="fs-id11044637"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mrow>     <m:mrow><m:mo>(</m:mo>      <m:mrow>       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>      </m:mrow>     <m:mo>)</m:mo></m:mrow>    </m:mrow>    <m:mn>2</m:mn>   </m:msup>   <m:mo>=</m:mo><m:mn>3</m:mn>  </m:mrow>  </m:semantics></m:math></para>
</problem><solution id="zip-id5440007"><para id="zip-id5440008"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>±</m:mo><m:msqrt>
    <m:mn>3</m:mn>
   </m:msqrt>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id11018156"><problem id="fs-id11018157"><para id="fs-id11018158"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mrow>     <m:mrow><m:mo>(</m:mo>      <m:mrow>       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>      </m:mrow>     <m:mo>)</m:mo></m:mrow>    </m:mrow>    <m:mn>2</m:mn>   </m:msup>   <m:mo>=</m:mo><m:mn>4</m:mn>  </m:mrow>  </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id11043650"><problem id="fs-id11043652"><para id="fs-id11043653"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mrow>     <m:mrow><m:mo>(</m:mo>      <m:mrow>       <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>      </m:mrow>     <m:mo>)</m:mo></m:mrow>    </m:mrow>    <m:mn>2</m:mn>   </m:msup>   <m:mo>=</m:mo><m:mn>8</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem><solution id="zip-id5491812"><para id="zip-id5491813"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>b</m:mi><m:mo>=</m:mo><m:mn>6</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt>
    <m:mn>2</m:mn>
   </m:msqrt>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id10685527"><problem id="fs-id10685528"><para id="fs-id10685529"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mrow>     <m:mrow><m:mo>(</m:mo>      <m:mrow>       <m:mn>3</m:mn><m:mo>−</m:mo><m:mi>x</m:mi>      </m:mrow>     <m:mo>)</m:mo></m:mrow>    </m:mrow>    <m:mn>2</m:mn>   </m:msup>   <m:mo>=</m:mo><m:mn>6</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id11035339"><problem id="fs-id11035340"><para id="fs-id11035341"><m:math> <m:semantics>  <m:mrow>   <m:mn>3</m:mn><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:msup>      <m:mi>x</m:mi>      <m:mn>2</m:mn>     </m:msup>     <m:mo>+</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>7</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow>  </m:mrow>   </m:semantics></m:math></para>
</problem><solution id="zip-id20401127"><para id="zip-id20401128"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>1</m:mn><m:mo>±</m:mo><m:msqrt>
      <m:mrow>
       <m:mn>34</m:mn>
      </m:mrow>
     </m:msqrt>
     
    </m:mrow>
    <m:mn>3</m:mn>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id11039842"><problem id="fs-id11039843"><para id="fs-id11039844"><m:math> <m:semantics>  <m:mrow>   <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:msup>      <m:mi>y</m:mi>      <m:mn>2</m:mn>     </m:msup>     <m:mo>−</m:mo><m:mn>3</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow>  </m:mrow>  </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id11037122"><problem id="fs-id11037123"><para id="fs-id11037124"><m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo><m:mn>4</m:mn><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:msup>      <m:mi>a</m:mi>      <m:mn>2</m:mn>     </m:msup>     <m:mo>+</m:mo><m:mn>2</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mn>5</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem><solution id="zip-id8769973"><para id="zip-id8769974">No real number solution.</para></solution></exercise><exercise id="fs-id11021392"><problem id="fs-id11021393"><para id="fs-id11021394"><m:math> <m:semantics>  <m:mrow>   <m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:msup>      <m:mi>x</m:mi>      <m:mn>2</m:mn>     </m:msup>     <m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise></section><section id="fs-id11017684"><title>Exercises For Review</title><exercise id="fs-id11017690"><problem id="fs-id11017691"><para id="fs-id11017692"><emphasis>(<link document="m21897"/>)</emphasis> Simplify <m:math>
					<m:semantics>
						<m:mrow>
							<m:msup>
								<m:mrow>
									<m:mrow><m:mo>(</m:mo>
										<m:mrow>
											<m:mfrac>
												<m:mrow>
													<m:msup>
														<m:mi>x</m:mi>
														<m:mn>8</m:mn>
													</m:msup>
													<m:msup>
														<m:mi>y</m:mi>
														<m:mn>7</m:mn>
													</m:msup>
													<m:msup>
														<m:mi>z</m:mi>
														<m:mn>5</m:mn>
													</m:msup>
												</m:mrow>
												<m:mrow>
													<m:msup>
														<m:mi>x</m:mi>
														<m:mn>4</m:mn>
													</m:msup>
													<m:msup>
														<m:mi>y</m:mi>
														<m:mn>6</m:mn>
													</m:msup>
													<m:msup>
														<m:mi>z</m:mi>
														<m:mn>2</m:mn>
													</m:msup>
												</m:mrow>
											</m:mfrac>
										</m:mrow>
										<m:mo>)</m:mo></m:mrow>
								</m:mrow>
								<m:mn>2</m:mn>
							</m:msup>
							<m:mo>.</m:mo>
						</m:mrow>
					</m:semantics></m:math></para>
		</problem><solution id="zip-id15955722"><para id="zip-id15955723"><m:math>
					<m:semantics>
						<m:mrow>
							<m:msup>
								<m:mi>x</m:mi>
								<m:mn>8</m:mn>
							</m:msup>
							<m:msup>
								<m:mi>y</m:mi>
								<m:mn>2</m:mn>
							</m:msup>
							<m:msup>
								<m:mi>z</m:mi>
								<m:mn>6</m:mn>
							</m:msup>
						</m:mrow>
					</m:semantics>
				</m:math>
			</para></solution></exercise><exercise id="fs-id11037233"><problem id="fs-id11037234"><para id="fs-id11037235"><emphasis>(<link document="m21882"/>)</emphasis> Write <m:math>
					<m:semantics>
						<m:mrow>
							<m:mn>4</m:mn><m:msup>
								<m:mi>a</m:mi>
								<m:mrow>
									<m:mo>−</m:mo><m:mn>6</m:mn>
								</m:mrow>
							</m:msup>
							<m:msup>
								<m:mi>b</m:mi>
								<m:mn>2</m:mn>
							</m:msup>
							<m:msup>
								<m:mi>c</m:mi>
								<m:mn>3</m:mn>
							</m:msup>
							<m:msup>
								<m:mi>a</m:mi>
								<m:mn>5</m:mn>
							</m:msup>
							<m:msup>
								<m:mi>b</m:mi>
								<m:mrow>
									<m:mo>−</m:mo><m:mn>3</m:mn>
								</m:mrow>
							</m:msup>
						</m:mrow>
					</m:semantics></m:math> so that only positive exponents appear.</para>
		</problem></exercise><exercise id="fs-id11035453"><problem id="fs-id11035454"><para id="fs-id11035455"><emphasis>(<link document="m21852"/>)</emphasis> Find the product: <m:math>
					<m:semantics>
						<m:mrow>
							<m:mrow><m:mo>(</m:mo>
								<m:mrow>
									<m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>7</m:mn>
								</m:mrow>
								<m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
								<m:mrow>
									<m:mn>3</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
								</m:mrow>
								<m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
						</m:mrow>
					</m:semantics></m:math></para>
		</problem><solution id="zip-id18650195"><para id="zip-id18650196"><m:math>
					<m:semantics>
						<m:mrow>
							<m:mn>6</m:mn><m:msup>
								<m:mi>y</m:mi>
								<m:mn>2</m:mn>
							</m:msup>
							<m:mo>+</m:mo><m:mn>19</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
						</m:mrow>
					</m:semantics>
				</m:math>
			</para></solution></exercise><exercise id="fs-id11024812"><problem id="fs-id11024814"><para id="fs-id11024815"><emphasis>(<link document="m21957"/>)</emphasis> Simplify: <m:math>
					<m:semantics>
						<m:mrow>
							<m:msqrt>
								<m:mrow>
									<m:mn>80</m:mn>
								</m:mrow>
							</m:msqrt>
							<m:mo>−</m:mo><m:msqrt>
								<m:mrow>
									<m:mn>45</m:mn>
								</m:mrow>
							</m:msqrt>
							<m:mo>.</m:mo>
						</m:mrow>
					</m:semantics></m:math></para>
		</problem></exercise><exercise id="fs-id11040629"><problem id="fs-id11040630"><para id="fs-id11040632"><emphasis>(<link document="m21935"/>)</emphasis> Solve <m:math>
					<m:semantics>
						<m:mrow>
							<m:msup>
								<m:mi>x</m:mi>
								<m:mn>2</m:mn>
							</m:msup>
							<m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>12</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
						</m:mrow>
					</m:semantics></m:math> by completing the square.</para>
		</problem><solution id="zip-id5368351"><para id="zip-id5368352"><m:math>
					<m:semantics>
						<m:mrow>
							<m:mi>x</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mtext> </m:mtext><m:mn>6</m:mn>
						</m:mrow>
					</m:semantics>
				</m:math>
			</para></solution></exercise></section>  </content></document>