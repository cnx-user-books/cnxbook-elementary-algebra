>      <m:mrow>       <m:msup>        <m:mi>b</m:mi>        <m:mn>2</m:mn>       </m:msup>       <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>a</m:mi><m:mi>c</m:mi>      </m:mrow>     </m:msqrt>         </m:mrow>    <m:mrow>     <m:mn>2</m:mn><m:mi>a</m:mi>    </m:mrow>   </m:mfrac>     </m:mrow>   </m:semantics></m:math></item><item>Substitute.<newline/><newline/><m:math> <m:semantics>  <m:mrow>   <m:mtable columnalign="left">    <m:mtr columnalign="left">     <m:mtd columnalign="left">      <m:mi>x</m:mi>     </m:mtd>     <m:mtd columnalign="left">      <m:mo>=</m:mo>     </m:mtd>     <m:mtd columnalign="left">      <m:mrow>       <m:mfrac>        <m:mrow>         <m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>          <m:mrow>           <m:mo>−</m:mo><m:mn>6</m:mn>          </m:mrow>         <m:mo>)</m:mo></m:mrow><m:mo>±</m:mo><m:msqrt>          <m:mrow>           <m:msup>            <m:mrow>             <m:mrow><m:mo>(</m:mo>              <m:mrow>               <m:mo>−</m:mo><m:mn>6</m:mn>              </m:mrow>             <m:mo>)</m:mo></m:mrow>            </m:mrow>            <m:mn>2</m:mn>           </m:msup>           <m:mo>−</m:mo><m:mn>4</m:mn><m:mrow><m:mo>(</m:mo>            <m:mn>1</m:mn>           <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>            <m:mrow>             <m:mo>−</m:mo><m:mn>1</m:mn>            </m:mrow>           <m:mo>)</m:mo></m:mrow>          </m:mrow>         </m:msqrt>                 </m:mrow>        <m:mrow>         <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>          <m:mn>1</m:mn>         <m:mo>)</m:mo></m:mrow>        </m:mrow>       </m:mfrac>             </m:mrow>     </m:mtd>    </m:mtr>    <m:mtr columnalign="left">     <m:mtd columnalign="left">      <m:mrow/>     </m:mtd>     <m:mtd columnalign="left">      <m:mo>=</m:mo>     </m:mtd>     <m:mtd columnalign="left">      <m:mrow>       <m:mfrac>        <m:mrow>         <m:mn>6</m:mn><m:mo>±</m:mo><m:msqrt>          <m:mrow>           <m:mn>36</m:mn><m:mo>+</m:mo><m:mn>4</m:mn>          </m:mro