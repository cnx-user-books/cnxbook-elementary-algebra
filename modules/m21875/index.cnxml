											</m:mrow>
											<m:mo>)</m:mo></m:mrow>
									</m:mrow>
									<m:mo>]</m:mo></m:mrow><m:mrow><m:mo>[</m:mo>
									<m:mrow>
										<m:mo>−</m:mo><m:mn>2</m:mn>
									</m:mrow>
									<m:mo>]</m:mo></m:mrow>
							</m:mrow>
