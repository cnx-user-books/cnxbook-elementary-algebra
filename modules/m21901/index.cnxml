</problem>	</exercise><exercise id="eip-86"><problem id="eip-909">			<para id="eip-335">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>5</m:mn><m:msup>								<m:mi>x</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:msup>								<m:mi>y</m:mi>								<m:mn>3</m:mn>							</m:msup>							<m:mi>z</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:msup>								<m:mi>x</m:mi>								<m:mn>3</m:mn>							</m:msup>							<m:mi>y</m:mi><m:mi>w</m:mi><m:mo>−</m:mo><m:mn>10</m:mn><m:msup>								<m:mi>y</m:mi>								<m:mn>3</m:mn>							</m:msup>							<m:msup>								<m:mi>z</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:mo>−</m:mo><m:mn>6</m:mn><m:mi>w</m:mi><m:mi>x</m:mi><m:mi>y</m:mi><m:mi>z</m:mi>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
