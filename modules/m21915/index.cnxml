   <m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext>length</m:mtext><m:mo>=</m:mo><m:mtext> </m:mtext><m:mfrac>
    <m:mn>2</m:mn>
    <m:mn>3</m:mn>
   </m:mfrac>
   <m:msqrt>
    <m:mrow>
     <m:mn>21</m:mn>
    </m:mrow>
