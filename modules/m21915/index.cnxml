mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:mfrac>											<m:mrow>												<m:mo>−</m:mo><m:mn>1</m:mn><m:mo>±</m:mo><m:msqrt>													<m:mrow>														<m:mn>1</m:mn><m:mo>+</m:mo><m:mn>624</m:mn>													</m:mrow>												</m:msqrt>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:mfrac>											<m:mrow>												<m:mo>−</m:mo><m:mn>1</m:mn><m:mo>±</m:mo><m:mn>25</m:mn>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:mfrac>											<m:mrow>												<m:mo>−</m:mo><m:mn>1</m:mn><m:mo>±</m:mo><m:mn>25</m:mn>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>										<m:mo>=</m:mo><m:mfrac>											<m:mrow>												<m:mn>24</m:mn>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>										<m:mo>=</m:mo><m:mn>12</m:mn><m:mtext> </m:mtext><m:mtext>and</m:mtext><m:mtext> </m:mtext><m:mfrac>											<m:mrow>												<m:mo>−</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>25</m:mn>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>										<m:mo>=</m:mo><m:mfrac>											<m:mrow>												<m:mo>−</m:mo><m:mn>26</m:mn>											</m:mrow>											<m:mn>2</m:mn>										</m:mfrac>										<m:mo>=</m:mo><m:mo>−</m:mo><m:mn>13</m:mn>									</m:mrow>								</m:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign