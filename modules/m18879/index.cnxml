</exercise>
<exercise id="fs-id7081967"><problem id="fs-id7081968"><para id="fs-id7081969"><m:math>
 <m:semantics>
  <m:mrow>
   <m:msqrt>
    <m:mrow>
     <m:mn>3</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
    </m:mrow>
   </m:msqrt>
   
  </m:mrow>
 </m:semantics>
</m:math>
</para></problem>
