					<m:semantics>
						<m:mrow>
							<m:mn>4</m:mn><m:mo>&lt;</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn><m:mo>&lt;</m:mo><m:mn>12</m:mn>
