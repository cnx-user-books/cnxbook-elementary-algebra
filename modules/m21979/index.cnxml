	</exercise><exercise id="eip-127"><problem id="eip-820">
			<para id="eip-439">
				<m:math>
					<m:semantics>
						<m:mrow>
							<m:mo>−</m:mo><m:mn>3</m:mn><m:mo>&lt;</m:mo><m:mn>7</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>&lt;</m:mo><m:mn>18</m:mn>
