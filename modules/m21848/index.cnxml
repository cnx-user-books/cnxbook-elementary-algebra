      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>5</m:mn><m:mo>,</m:mo><m:mn>7</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    
   </m:mtable>
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution>	</exercise><exercise id="eip-886"><problem id="eip-514">			<para id="eip-703">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>16</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>21</m:mn>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
</problem>	</exercise><exercise id="eip-485"><problem id="eip-62">			<para id="eip-728">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>4</m:mn><m:msup>								<m:mi>x</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:mo>+</m:mo><m:mn>9</m:mn>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
