      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>9</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    
   </m:mtable>
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution>	</exercise><exercise id="eip-612"><problem id="eip-790">			<para id="eip-59">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>7</m:mn><m:msup>								<m:mi>y</m:mi>								<m:mn>3</m:mn>							</m:msup>							<m:mo>+</m:mo><m:mn>8</m:mn>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
</problem>	</exercise><exercise id="eip-831"><problem id="eip-671">			<para id="eip-41">				<m:math>					<m:semantics>						<m:mrow>							<m:msup>								<m:mi>a</m:mi>								<m:mn>4</m:mn>							</m:msup>							<m:mo>+</m:mo><m:mn>1</m:mn>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
</problem><solution id="zip-id17035148"><para id="zip-id17035149"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mtable columnalign="left">
