</problem>	<solution id="eip-774">		<para id="eip-109">linear</para>	</solution></exercise><exercise id="eip-145"><problem id="eip-510">		<para id="eip-583"><m:math>		<m:semantics>			<m:mrow>				<m:mn>5</m:mn><m:msup>					<m:mi>x</m:mi>					<m:mn>7</m:mn>				</m:msup>				<m:mo>=</m:mo><m:mn>3</m:mn><m:msup>					<m:mi>x</m:mi>					<m:mn>5</m:mn>				</m:msup>				<m:mo>−</m:mo><m:mn>2</m:mn><m:msup>					<m:mi>x</m:mi>					<m:mn>8</m:mn>				</m:msup>				<m:mo>+</m:mo><m:mn>11</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>9</m:mn>			</m:mrow>			<m:annotation encoding="MathType-MTEF">			</m:annotation>		</m:semantics>	</m:math></para>	
