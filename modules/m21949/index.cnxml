	<exercise id="fs-id3764267"><problem id="fs-id3764268"><para id="fs-id3764269"><m:math>
					<m:semantics>
						<m:mrow>
							<m:msup>
								<m:mi>m</m:mi>
								<m:mn>4</m:mn>
							</m:msup>
							<m:mo>+</m:mo><m:mn>2</m:mn><m:msup>
								<m:mi>m</m:mi>
								<m:mn>3</m:mn>
							</m:msup>
							<m:mo>−</m:mo><m:mn>8</m:mn><m:msup>
								<m:mi>m</m:mi>
								<m:mn>2</m:mn>
							</m:msup>
							<m:mo>−</m:mo><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
						</m:mrow>
					</m:semantics>
				</m:math> by <m:math>
					<m:semantics>
