										<m:msup>
											<m:mi>x</m:mi>
											<m:mn>2</m:mn>
										</m:msup>
										<m:mo>−</m:mo><m:mn>9</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>18</m:mn>
