								<m:mi>y</m:mi><m:mo>≠</m:mo><m:mfrac>
									<m:mn>1</m:mn>
									<m:mn>6</m:mn>
								</m:mfrac>
								<m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
									<m:mn>5</m:mn>
									<m:mn>2</m:mn>
								</m:mfrac>
