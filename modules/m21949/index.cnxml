									</m:mrow>
									<m:mrow>
										<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></problem></exercise>
		<exercise id="fs-id8221205"><problem id="fs-id4983929"><para id="fs-id4983930"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mfrac>
									<m:mrow>
										<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></problem><solution id="zip-id19524790"><para id="zip-id19526879"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mi>x</m:mi><m:mo>≠</m:mo><m:mfrac>
									<m:mn>5</m:mn>
									<m:mn>2</m:mn>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></solution></exercise>
