									</m:mrow>
									<m:mrow>
										<m:mn>9</m:mn><m:mi>r</m:mi><m:mrow><m:mo>(</m:mo>
											<m:mrow>
												<m:mn>2</m:mn><m:mi>r</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
											</m:mrow>
											<m:mo>)</m:mo></m:mrow>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></problem></exercise>
		<exercise id="fs-id10410622"><problem id="fs-id10410623"><para id="fs-id10410624"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mfrac>
									<m:mi>s</m:mi>
									<m:mrow>
										<m:mi>s</m:mi><m:mrow><m:mo>(</m:mo>
											<m:mrow>
												<m:mi>s</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
											</m:mrow>
											<m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
											<m:mrow>
												<m:mn>4</m:mn><m:mi>s</m:mi><m:mo>+</m:mo><m:mn>7</m:mn>
											</m:mrow>
											<m:mo>)</m:mo></m:mrow>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></problem><solution id="zip-id16812138"><para id="zip-id17706747"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mi>s</m:mi><m:mo>≠</m:mo><m:mo>−</m:mo><m:mn>8</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
									<m:mn>7</m:mn>
									<m:mn>4</m:mn>
								</m:mfrac>
								<m:mo>,</m:mo><m:mn>0</m:mn>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></solution></exercise>
		<exercise id="fs-id8620071"><problem id="fs-id10659981"><para id="fs-id10659982"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mfrac>
									<m:mrow>
