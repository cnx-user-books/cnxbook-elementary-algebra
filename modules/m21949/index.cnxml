		<exercise id="fs-id10728629"><problem id="fs-id10728630"><para id="fs-id10728632"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mfrac>
									<m:mrow>
										<m:mn>3</m:mn><m:mi>m</m:mi>
									</m:mrow>
									<m:mrow>
										<m:mn>2</m:mn><m:mi>m</m:mi><m:mrow><m:mo>(</m:mo>
											<m:mrow>
												<m:mi>m</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
											</m:mrow>
											<m:mo>)</m:mo></m:mrow>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></problem><solution id="zip-id19279350"><para id="zip-id5660711"><m:math>
						<m:semantics>
							<m:mrow>
								<m:mi>m</m:mi><m:mo>≠</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
							</m:mrow>
						</m:semantics>
					</m:math>
				</para></solution></exercise>
		<exercise id="fs-id6321174"><problem id="fs-id6321175"><para id="fs-id6321176"><m:math>
						<m:semantics>
							<m:mrow>
