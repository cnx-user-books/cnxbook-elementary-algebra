   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mtext>So,</m:mtext><m:mtext> </m:mtext><m:mn>2</m:mn><m:mi>x</m:mi><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mn>4</m:mn><m:msup>
