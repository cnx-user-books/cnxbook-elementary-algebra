            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
