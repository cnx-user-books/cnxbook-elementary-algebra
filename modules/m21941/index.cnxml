<exercise id="fs-id28564103"><problem id="fs-id28564104">
<para id="fs-id28564106">
<m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>a</m:mi><m:msup>
      <m:mi>b</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>6</m:mn><m:msup>
      <m:mi>a</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>6</m:mn><m:mi>b</m:mi>
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>a</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>a</m:mi>
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>

</para></problem>
<solution id="fs-id28564250"><para id="fs-id28564252"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>a</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:msup>
      <m:mi>b</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:msup>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
       <m:mo>)</m:mo></m:mrow>
      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:msup>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
       <m:mo>)</m:mo></m:mrow>
      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>6</m:mn><m:mi>b</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:msup>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
       <m:mo>)</m:mo></m:mrow>
      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>a</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>a</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:msup>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
       <m:mo>)</m:mo></m:mrow>
      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>

</para></solution>
</exercise>
