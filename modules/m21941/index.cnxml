          <m:mrow>
           <m:msup>
            <m:mi>x</m:mi>
            <m:mn>3</m:mn>
           </m:msup>
           <m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           
          </m:mrow>
         </m:mfrac>
         <m:mo>,</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>2</m:mn><m:mi>x</m:mi>
