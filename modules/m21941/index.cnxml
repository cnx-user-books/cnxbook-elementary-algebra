          <m:mi>N</m:mi>
          <m:mrow>
           <m:mn>15</m:mn>
          </m:mrow>
         </m:mfrac>
         <m:mo>.</m:mo>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mtable columnalign="left">
         <m:mtr>
          <m:mtd>
           <m:mtext>The original denominator is 3 and the new </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>denominato is 15</m:mtext><m:mtext>. Divide the original </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>denominator into the new denominator and </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>multiply the numerator 8 by this result</m:mtext><m:mtext>.</m:mtext>
          </m:mtd>
         </m:mtr>
        </m:mtable>
        
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>15</m:mn><m:mo>÷</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mn>5</m:mn>
        </m:mrow>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mtext>Then,</m:mtext><m:mtext> </m:mtext><m:mn>8</m:mn><m:mo>⋅</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>40.</m:mn><m:mtext> </m:mtext><m:mtext>So,</m:mtext>
        </m:mrow>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mn>8</m:mn>
          <m:mn>3</m:mn>
         </m:mfrac>
         <m:mo>=</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>40</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mn>15</m:mn>
          </m:mrow>
         </m:mfrac>
         <m:mtext> </m:mtext><m:mtext>and</m:mtext><m:mtext> </m:mtext><m:mi>N</m:mi><m:mo>=</m:mo><m:mn>40.</m:mn>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext>Check by reducing</m:mtext><m:mfrac>
