   <m:mtable columnalign="left">
    <m:mtr>
    <m:mtd>
     <m:mtext>The original denominator is </m:mtext><m:mn>5</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>2</m:mn>
     </m:msup>
     <m:mi>y</m:mi><m:mtext> and the new </m:mtext>
    </m:mtd>
    </m:mtr>
    <m:mtr>
    <m:mtd>
     <m:mtext>denominator is </m:mtext><m:mn>20</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>5</m:mn>
     </m:msup>
     <m:msup>
     <m:mi>y</m:mi>
     <m:mn>4</m:mn>
     </m:msup>
