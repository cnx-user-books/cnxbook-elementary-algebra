    </m:mtd>
    </m:mtr>
    <m:mtr>
    <m:mtd>
     <m:mtext>multiply the numerator </m:mtext><m:mn>2</m:mn><m:mi>x</m:mi><m:mtext> by this result</m:mtext><m:mtext>.</m:mtext>
    </m:mtd>
    </m:mtr>
   </m:mtable>
   
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mfrac>
    <m:mrow>
     <m:mn>20</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>5</m:mn>
     </m:msup>
     <m:msup>
     <m:mi>y</m:mi>
     <m:mn>4</m:mn>
     </m:msup>
     
    </m:mrow>
    <m:mrow>
