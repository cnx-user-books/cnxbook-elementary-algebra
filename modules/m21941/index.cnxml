    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow>
     <m:mtext>The new denominator divided by the original denominator is </m:mtext>
    </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow>
     <m:mfrac>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
     <m:mrow>
      <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
     </m:mrow>
     </m:mfrac>
     <m:mo>=</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
    </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow>
     <m:mtext>Multiply </m:mtext><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>a</m:mi><m:mtext> </m:mtext><m:mtext>by</m:mtext><m:mtext> </m:mtext><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>7.</m:mn>
    </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
    <m:mrow>
     <m:mo>−</m:mo><m:mn>6</m:mn><m:mi>a</m:mi><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>6</m:mn><m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>42</m:mn><m:mi>a</m:mi>
    </m:mrow>
    </m:mtd>
   </m:mtr>
   
   </m:mtable>
  </m:mtd>
  </m:mtr>
  <m:mtr>
  <m:mtd>
   <m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>6</m:mn><m:mi>a</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
   </m:mrow>
   </m:mfrac>
   <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>6</m:mn><m:msup>
    <m:mi>a</m:mi>
