          </m:mrow>
         </m:mfrac>
         <m:mo>,</m:mo><m:mfrac>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
          <m:mrow>
           <m:msup>
            <m:mrow>
