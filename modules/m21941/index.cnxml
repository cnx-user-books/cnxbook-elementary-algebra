<example id="fs-id29787795"><para id="fs-id29787796"><m:math display="block">
 <m:semantics>
  <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mtable columnalign="left">
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
