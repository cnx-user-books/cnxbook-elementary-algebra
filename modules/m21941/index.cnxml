    </m:mrow>
    </m:mfrac>
    <m:mo>.</m:mo>
   </m:mrow>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mtext>By inspection, the LCD is </m:mtext><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
   </m:mrow>
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mtext>Rewrite each fraction with new denominator </m:mtext><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
   </m:mrow>
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mfrac>
    <m:mrow/>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
    <m:mrow/>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    
   </m:mrow>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mtable columnalign="left">
    <m:mtr>
    <m:mtd>
     <m:mtext>The denominator of the first rational expression has been multiplied </m:mtext>
    </m:mtd>
    </m:mtr>
    <m:mtr>
    <m:mtd>
     <m:mtext>by </m:mtext><m:mi>b</m:mi><m:mtext> </m:mtext><m:mo>+</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext>so the numerator </m:mtext><m:mn>4</m:mn><m:mi>b</m:mi><m:mtext> must be multiplied by </m:mtext><m:mi>b</m:mi><m:mtext> </m:mtext><m:mo>+</m:mo><m:mn>3.</m:mn><m:mtext> </m:mtext>
    </m:mtd>
    </m:mtr>
   </m:mtable>
   
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mn>4</m:mn><m:mi>b</m:mi><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>4</m:mn><m:msup>
    <m:mi>b</m:mi>
    <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>12</m:mn><m:mi>b</m:mi>
   </m:mrow>
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mfrac>
    <m:mrow>
     <m:mn>4</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>12</m:mn><m:mi>b</m:mi>
    </m:mrow>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
    <m:mrow/>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    
   </m:mrow>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mtable columnalign="left">
    <m:mtr>
    <m:mtd>
     <m:mtext>The denominator of the second rational expression has been multiplied </m:mtext>
    </m:mtd>
    </m:mtr>
    <m:mtr>
    <m:mtd>
     <m:mtext>by </m:mtext><m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mtext>, so the numerator </m:mtext><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mtext> must be multiplied by </m:mtext><m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1.</m:mn>
    </m:mtd>
    </m:mtr>
   </m:mtable>
   
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
    <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
    <m:mi>b</m:mi>
    <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>b</m:mi>
   </m:mrow>
   </m:mtd>
  </m:mtr>
  <m:mtr columnalign="left">
   <m:mtd columnalign="left">
   <m:mrow>
    <m:mfrac>
    <m:mrow>
     <m:mn>4</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>12</m:mn><m:mi>b</m:mi>
    </m:mrow>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
     <m:mi>b</m:mi>
     <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>b</m:mi>
    </m:mrow>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
     </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    </m:mfrac>
    
   </m:mrow>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
   <m:mtd columnalign="left">
   <m:mrow/>
   </m:mtd>
  </m:mtr>
  
  </m:mtable>
 </m:mrow>
 
 </m:semantics>
</m:math>

</para></example>
<example id="fs-id27635846"><para id="fs-id27635847"><m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>6</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>15</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mo>−</m:mo><m:mn>7</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>12</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>We first find the LCD</m:mtext><m:mo>.</m:mo><m:mtext> Factor</m:mtext><m:mo>.</m:mo><m:mtext> </m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>6</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mtable columnalign="left">
       <m:mtr>
        <m:mtd>
         <m:mtext>The LCD is </m:mtext><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo><m:mtext> </m:mtext><m:mtext>Rewrite each of these </m:mtext>
        </m:mtd>
       </m:mtr>
       <m:mtr>
        <m:mtd>
         <m:mtext>fractions with new denominator </m:mtext><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
        </m:mtd>
       </m:mtr>
      </m:mtable>
      
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow/>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
        <m:mrow/>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mtable columnalign="left">
       <m:mtr>
        <m:mtd>
         <m:mtext>By comparing the denominator of the first fraction with the LCD </m:mtext>
        </m:mtd>
       </m:mtr>
       <m:mtr>
        <m:mtd>
         <m:mtext>we see that we must multiply the numerator </m:mtext><m:mn>6</m:mn><m:mi>x</m:mi><m:mtext> by </m:mtext><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4.</m:mn>
        </m:mtd>
       </m:mtr>
      </m:mtable>
      
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>6</m:mn><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
        </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>6</m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>24</m:mn><m:mi>x</m:mi>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>6</m:mn><m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mo>−</m:mo><m:mn>24</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
        <m:mrow/>
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mtable columnalign="left">
       <m:mtr>
        <m:mtd>
         <m:mtext>By comparing the denominator of the second fraction with the LCD, </m:mtext>
        </m:mtd>
       </m:mtr>
       <m:mtr>
        <m:mtd>
         <m:mtext>we see that we must multiply the numerator </m:mtext><m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mtext> by </m:mtext><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5.</m:mn>
        </m:mtd>
       </m:mtr>
      </m:mtable>
      
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
