				<m:mtr>
					<m:mtd>
						<m:mtable columnalign="left">
							<m:mtr columnalign="left">
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mfrac>
											<m:mrow>
												<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
											</m:mrow>
											<m:mrow>
												<m:mi>b</m:mi><m:mo>−</m:mo><m:mi>a</m:mi>
											</m:mrow>
										</m:mfrac>
										<m:mo>.</m:mo>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mtable columnalign="left">
										<m:mtr>
											<m:mtd>
												<m:mtext>The numerator and denominator have the same terms but they </m:mtext>
											</m:mtd>
										</m:mtr>
										<m:mtr>
											<m:mtd>
												<m:mtext>occur with opposite signs</m:mtext><m:mtext>. Factor </m:mtext><m:mo>−</m:mo><m:mtext>1 from the denominator</m:mtext><m:mtext>. </m:mtext>
											</m:mtd>
										</m:mtr>
									</m:mtable>
								</m:mtd>
							</m:mtr>
						</m:mtable>
					</m:mtd>
				</m:mtr>
				<m:mtr>
					<m:mtd>
						<m:mfrac>
							<m:mrow>
								<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
							</m:mrow>
							<m:mrow>
								<m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>
									<m:mrow>
										<m:mo>−</m:mo><m:mi>b</m:mi><m:mo>+</m:mo><m:mi>a</m:mi>
									</m:mrow>
									<m:mo>)</m:mo></m:mrow>
							</m:mrow>
						</m:mfrac>
						<m:mo>=</m:mo><m:mfrac>
							<m:mrow>
								<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
							</m:mrow>
							<m:mrow>
								<m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>
									<m:mrow>
										<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
									</m:mrow>
									<m:mo>)</m:mo></m:mrow>
							</m:mrow>
						</m:mfrac>
						<m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
							<m:mrow>
								<m:menclose notation="updiagonalstrike">
									<m:mrow>
										<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
									</m:mrow>
								</m:menclose>
							</m:mrow>
							<m:mrow>
								<m:menclose notation="updiagonalstrike">
									<m:mrow>
										<m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
									</m:mrow>
								</m:menclose>
							</m:mrow>
						</m:mfrac>
						<m:mo>=</m:mo><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>a</m:mi><m:mo>≠</m:mo><m:mi>b</m:mi>
					</m:mtd>
				</m:mtr>
			</m:mtable>
		</m:semantics>
	</m:math>
</para></example>


