    <m:mi>x</m:mi>
    <m:mn>3</m:mn>
   </m:msup>
   <m:mo>−</m:mo><m:mn>12</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>68</m:mn><m:mo>−</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>407</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
   </m:semantics>
</m:math>
</para></example>
</section>
