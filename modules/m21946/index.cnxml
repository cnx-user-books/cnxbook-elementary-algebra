	<section id="fs-id45735290">
 <title>Practice Set A</title>
<para id="fs-id45735295">Perform the following divisions. </para>


<exercise id="fs-id45735298">
<problem id="fs-id45735300"><para id="fs-id45735301"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
    <m:mi>x</m:mi>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></problem>
<solution id="fs-id45735344"><para id="fs-id45735345"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mfrac>
    <m:mn>1</m:mn>
    <m:mi>x</m:mi>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution>
</exercise>





<exercise id="fs-id45735379">
<problem id="fs-id45735381"><para id="fs-id45735382"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
