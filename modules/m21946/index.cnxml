</section>


<section id="fs-id45136502">
 <title>Sample Set C</title>
 <example id="fs-id45136507"><para id="fs-id45136508"><m:math>
 <m:semantics>
  <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mtext>Divide </m:mtext><m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mtext> </m:mtext><m:mtext>by </m:mtext><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6.</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtable columnalign="left">
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mn>2</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>3</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mtable columnalign="left">
         <m:mtr>
          <m:mtd>
           <m:mtext>Notice that the </m:mtext><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mtext> term in the numerator is missing</m:mtext><m:mtext>. </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>We can avoid any confusion by writing</m:mtext>
          </m:mtd>
         </m:mtr>
        </m:mtable>
        
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mn>2</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>3</m:mn>
           </m:msup>
           <m:mo>+</m:mo><m:mn>0</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mtext>Divide, multiply, and subtract</m:mtext><m:mtext>.</m:mtext>
        </m:mrow>
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
  </m:mtable>
 </m:semantics>
</m:math><newline/><newline/>
<media id="fs-id45136781" alt="Steps of long division showing the quantity x plus six dividing the quantity two x cubed plus zero x squared minus four x minus plus one. See the longdesc for a full description">
  <image mime-type="image/jpeg" src="../../media/C07_S7-9_P87_001.jpg" longdesc="C07_S7-9_P85_002.html"/>
</media>

<newline/><newline/><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
    </m:mrow>
