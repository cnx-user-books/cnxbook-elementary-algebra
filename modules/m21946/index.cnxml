              <m:mn>6</m:mn>
             </m:msup>
             <m:mo>−</m:mo><m:mn>9</m:mn><m:msup>
              <m:mi>b</m:mi>
              <m:mn>4</m:mn>
             </m:msup>
             <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
            </m:mrow>
            <m:mrow>
             <m:mo>−</m:mo><m:mn>4</m:mn><m:msup>
              <m:mi>b</m:mi>
              <m:mn>2</m:mn>
             </m:msup>
             
            </m:mrow>
           </m:mfrac>
           <m:mo>.</m:mo>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="left">
          <m:mrow/>
         </m:mtd>
         <m:mtd columnalign="left">
          <m:mrow>
           <m:mtext>Divide every term of</m:mtext>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        
       </m:mtable><m:mn>4</m:mn><m:msup>
        <m:mi>b</m:mi>
        <m:mn>6</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>9</m:mn><m:msup>
        <m:mi>b</m:mi>
        <m:mn>4</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mtext> </m:mtext><m:mtext>by</m:mtext><m:mo>−</m:mo><m:mn>4</m:mn><m:msup>
        <m:mi>b</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
