</problem>	</exercise><exercise id="eip-316"><problem id="eip-640">			<para id="eip-304">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>5</m:mn><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mo stretchy="false">)</m:mo>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
</problem><solution id="zip-id8436174"><para id="zip-id2808456"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>5</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>30</m:mn>
  </m:mrow>
  
 </m:semantics>
</m:math>
