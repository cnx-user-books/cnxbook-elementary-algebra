:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:mrow><m:mo>[</m:mo>											<m:mrow>												<m:msup>													<m:mi>x</m:mi>													<m:mn>2</m:mn>												</m:msup>												<m:mo>+</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn>											</m:mrow>											<m:mo>]</m:mo></m:mrow><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:msup>											<m:mi>x</m:mi>											<m:mn>2</m:mn>										</m:msup>										<m:mo>⋅</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:msup>											<m:mi>x</m:mi>											<m:mn>2</m:mn>										</m:msup>										<m:mo>⋅</m:mo><m:mn>5</m:mn><m:mo>+</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>⋅</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>⋅</m:mo><m:mn>5</m:mn><m:mo>+</m:mo><m:mn>25</m:mn><m:mo>⋅</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn><m:mo>⋅</m:mo><m:mn>5</m:mn>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:msup>											<m:mi>x</m:mi>											<m:mn>3</m:mn>										</m:msup>										<m:mo>+</m:mo><m:mn>5</m:mn><m:msup>											<m:mi>x</m:mi>											<m:mn>2</m:mn>										</m:msup>										<m:mo>+</m:mo><m:mn>10</m:mn><m:msup>											<m:mi>x</m:mi>											<m:mn>2</m:mn>										</m:msup>										<m:mo>+</m:mo><m:mn>50</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>125</m:mn>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>							</m:mtr>							<m:mtr columnalign="left">								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>								<m:mtd columnalign="left">									<m:mo>=</m:mo>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow>										<m:msup>											<m:mi>x</m:mi>											<m:mn>3</m:mn>										</m:msup>										<m:mo>+</m:mo><m:mn>15</m:mn><m:msup>											<m:mi>x</m:mi>											<m:mn>2</m:mn>										</m:msup>										<m:mo>+</m:mo><m:mn>75</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>125</m:mn>									</m:mrow>								</m:mtd>								<m:mtd columnalign="left">									<m:mrow/>								</m:mtd>							</m:mtr>						</m:mtable>					</m:mrow>					<m:annotation encoding="MathType-MTEF">					</m:annotation>				</m:semantics>			</m:math>		</para>	</example></section><section id="eip-877"><title>Practice Set C</title><para id="eip-608">Find the following products and simplify.</para><exercise id="eip-504"><problem id="eip-568">		<para id="eip-636">    <m:math> <m:semantics>  <m:mrow>   <m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math>  </para>	
</problem>	<solution id="eip-286">		<para id="eip-169"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mi>a</m:mi>    <m:mn>2</m:mn>   </m:msup>   <m:mo>+</m:mo><m:mn>5</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math></para>	</solution></exercise><exercise id="eip-776"><problem id="eip-23">		<para id="eip-465">    <m:math> <m:semantics>  <m:mrow>   <m:mo stretchy="false">(</m:mo><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>9</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math>  </para>	
</problem>	<solution id="eip-66">		<para id="eip-987"><m:math> <m:semantics>  <m:mrow>   <m:msup>    <m:mi>m</m:mi>    <m:mn>2</m:mn>   </m:msup>   <m:mo>−</m:mo><m:mn>11</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>18</m:mn>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math></para>	</solution></exercise><exercise id="eip-503"><problem id="eip-970">		<para id="eip-273">    <m:math> <m:semantics>  <m:mrow>   <m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math>  </para>	
</problem>	<solution id="eip-459">		<para id="eip-260"><m:math> <m:semantics>  <m:mrow>   <m:mn>2</m:mn><m:msup>    <m:mi>x</m:mi>    <m:mn>2</m:mn>   </m:msup>   <m:mo>+</m:mo><m:mn>14</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>20</m:mn>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math></para>	</solution></exercise><exercise id="eip-914"><problem id="eip-108">		<para id="eip-593">    <m:math> <m:semantics>  <m:mrow>   <m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo>  </m:mrow> <m:annotation encoding="MathType-MTEF"> </m:annotation> </m:semantics></m:math>  </para>	
