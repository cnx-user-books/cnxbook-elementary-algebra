</problem>	</exercise><exercise id="eip-236"><problem id="eip-362">			<para id="eip-131">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>6</m:mn><m:mo stretchy="false">(</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
