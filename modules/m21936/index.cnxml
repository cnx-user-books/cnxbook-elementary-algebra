<example id="fs-id25693391"><para id="fs-id25693392">
<m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>7</m:mn><m:mo>−</m:mo><m:mi>x</m:mi>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>5</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mtable columnalign="left">
       <m:mtr>
        <m:mtd columnalign="left">
         <m:mtext>The denominators are</m:mtext><m:mtext> </m:mtext><m:mi>n</m:mi><m:mi>e</m:mi><m:mi>a</m:mi><m:mi>r</m:mi><m:mi>l</m:mi><m:mi>y</m:mi><m:mtext> </m:mtext><m:mtext>the same.</m:mtext><m:mtext> They differ only in sign.</m:mtext><m:mtext> </m:mtext>
        </m:mtd>
       </m:mtr>
       <m:mtr>
        <m:mtd columnalign="left">
         <m:mtext>Our technique is to factor </m:mtext><m:mo>−</m:mo><m:mn>1</m:mn><m:mtext> from one of them.</m:mtext><m:mtext> </m:mtext>
        </m:mtd>
       </m:mtr>
      </m:mtable>
      
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>7</m:mn><m:mo>−</m:mo><m:mi>x</m:mi>
        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>Factor </m:mtext><m:mo>−</m:mo><m:mn>1</m:mn><m:mtext> from the first term</m:mtext><m:mtext>. </m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>7</m:mn><m:mo>−</m:mo><m:mi>x</m:mi>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>5</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>5</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>2</m:mn><m:mi>x</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
    </m:mtr>
    
   </m:mtable>
  </m:mrow>
 
 </m:semantics>
</m:math>
</para></example>

</section>
<section id="fs-id25694009">
      <title>Practice Set B</title>
	  <para id="fs-id25694015">Add or Subtract the following rational expressions.</para>
	  <exercise id="fs-id25694019">
<problem id="fs-id25694021"><para id="fs-id25694022"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
