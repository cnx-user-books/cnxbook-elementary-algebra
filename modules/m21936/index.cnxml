</para></section><section id="fs-id25730295">
      <title>Sample Set B</title>
	  <para id="fs-id25695936">Add or Subtract the following rational expressions.</para>
<example id="fs-id25695939"><para id="fs-id25695940">
<m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>4</m:mn><m:mi>a</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>3</m:mn><m:mi>y</m:mi>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>2</m:mn><m:mi>a</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>9</m:mn><m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       <m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>The denominators are </m:mtext><m:mi>n</m:mi><m:mi>o</m:mi><m:mi>t</m:mi><m:mtext> the same</m:mtext><m:mtext>.</m:mtext><m:mtext> </m:mtext><m:mtext>Find</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext><m:mtext> </m:mtext><m:mtext>LCD</m:mtext><m:mtext>.</m:mtext><m:mtext> </m:mtext><m:mtext>By</m:mtext><m:mtext> </m:mtext><m:mtext>inspection,</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext><m:mtext> </m:mtext><m:mtext>LCD</m:mtext><m:mtext> </m:mtext><m:mtext>is</m:mtext><m:mtext> </m:mtext><m:mn>9</m:mn><m:msup>
        <m:mi>y</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow/>
        <m:mrow>
         <m:mn>9</m:mn><m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:mn>2</m:mn><m:mi>a</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>9</m:mn><m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtable columnalign="left">
        <m:mtr columnalign="left">
         <m:mtd columnalign="left">
          <m:mrow>
           <m:mtext>The</m:mtext><m:mtext> </m:mtext><m:mtext>denominator</m:mtext><m:mtext> </m:mtext><m:mtext>of</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext><m:mtext> </m:mtext><m:mtext>first</m:mtext><m:mtext> </m:mtext><m:mtext>rational</m:mtext><m:mtext> </m:mtext><m:mtext>expression</m:mtext><m:mtext> </m:mtext><m:mtext>has</m:mtext><m:mtext> </m:mtext><m:mtext>been</m:mtext><m:mtext> </m:mtext><m:mtext>multiplied</m:mtext><m:mtext> </m:mtext><m:mtext>by</m:mtext><m:mtext> </m:mtext><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>,</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="left">
         <m:mtd columnalign="left">
          <m:mrow>
           <m:mtext>so</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext><m:mtext> </m:mtext><m:mtext>numerator</m:mtext><m:mtext> </m:mtext><m:mtext>must</m:mtext><m:mtext> </m:mtext><m:mtext>be</m:mtext><m:mtext> </m:mtext><m:mtext>multiplied</m:mtext><m:mtext> </m:mtext><m:mtext>by</m:mtext><m:mtext> </m:mtext><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>.</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="left">
         <m:mtd columnalign="left">
          <m:mrow>
           <m:mn>4</m:mn><m:mi>a</m:mi><m:mi>·</m:mi><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>12</m:mn><m:mi>a</m:mi><m:mi>y</m:mi>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        
       </m:mtable>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mn>12</m:mn><m:mi>a</m:mi><m:mi>y</m:mi>
        </m:mrow>
        <m:mrow>
         <m:mn>9</m:mn><m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
