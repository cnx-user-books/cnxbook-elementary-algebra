<exercise id="fs-id25694580">
<problem id="fs-id25694582"><para id="fs-id25694583"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>2</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
    </m:mrow>
    <m:mi>y</m:mi>
