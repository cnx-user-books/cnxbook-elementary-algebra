</exercise><exercise id="eip-634"><problem id="eip-674">
		<para id="eip-46">
<m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>4</m:mn><m:mi>p</m:mi><m:mn>6</m:mn><m:mi>q</m:mi><m:mi>r</m:mi><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
  </m:mrow>
