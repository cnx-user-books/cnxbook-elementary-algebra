													</m:mrow>
													<m:mo>)</m:mo></m:mrow>
											</m:mrow>
										</m:mfrac>
									</m:mrow>
								</m:mtd>
								<m:mtd>
									<m:mrow>
										<m:mtext>or</m:mtext>
									</m:mrow>
								</m:mtd>
								<m:mtd>
									<m:mrow>
										<m:mfrac>
											<m:mrow>
												<m:mo>-</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>1</m:mn>
											</m:mrow>
											<m:mrow>
												<m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi>
											</m:mrow>
										</m:mfrac>
									</m:mrow>
								</m:mtd>
							</m:mtr>
						</m:mtable>
					</m:mtd>
				</m:mtr>
			</m:mtable>
		</m:semantics>
	</m:math>
</para></example>
 </section>

