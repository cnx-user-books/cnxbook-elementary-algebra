         </m:menclose>
         <m:menclose notation="updiagonalstrike">
          <m:mi>a</m:mi>
         </m:menclose>
         
        </m:mrow>
       </m:mfrac>
       <m:mo>·</m:mo><m:mfrac>
        <m:mrow>
         <m:mover>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mrow>
             <m:mn>10</m:mn>
            </m:mrow>
           </m:menclose>
           
          </m:mrow>
          <m:mn>2</m:mn>
         </m:mover>
         <m:msup>
          <m:mi>a</m:mi>
          <m:mrow>
           <m:mover>
            <m:mrow>
             <m:menclose notation="updiagonalstrike">
              <m:mn>3</m:mn>
             </m:menclose>
             
            </m:mrow>
            <m:mn>2</m:mn>
           </m:mover>
           
          </m:mrow>
         </m:msup>
         
        </m:mrow>
        <m:mrow>
         <m:menclose notation="updiagonalstrike">
