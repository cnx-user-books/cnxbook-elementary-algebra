          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mn>6</m:mn>
           </m:menclose>
           
          </m:mrow>
          <m:mn>3</m:mn>
         </m:mover>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mn>2</m:mn>
           </m:menclose>
           
          </m:mrow>
         </m:msup>
         
        </m:mrow>
        <m:mrow>
         <m:menclose notation="updiagonalstrike">
