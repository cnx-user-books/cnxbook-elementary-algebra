															</m:mrow>
															<m:mo>)</m:mo></m:mrow>
													</m:mrow>
												</m:menclose>
												<m:mrow><m:mo>(</m:mo>
													<m:mrow>
														<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>1</m:mn>
													</m:mrow>
													<m:mo>)</m:mo></m:mrow>
											</m:mrow>
										</m:mfrac>
										<m:mo>·</m:mo><m:mfrac>
											<m:mrow>
												<m:menclose notation="updiagonalstrike">
													<m:mrow>
														<m:mrow><m:mo>(</m:mo>
															<m:mrow>
																<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>3</m:mn>
															</m:mrow>
															<m:mo>)</m:mo></m:mrow>
													</m:mrow>
												</m:menclose>
												<m:mrow><m:mo>(</m:mo>
													<m:mrow>
														<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
													</m:mrow>
													<m:mo>)</m:mo></m:mrow>
											</m:mrow>
