														<m:mrow><m:mo>(</m:mo>
															<m:mrow>
																<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2</m:mn>
															</m:mrow>
															<m:mo>)</m:mo></m:mrow>
													</m:mrow>
												</m:menclose>
											</m:mrow>
											<m:mrow>
												<m:menclose notation="updiagonalstrike">
													<m:mrow>
														<m:mrow><m:mo>(</m:mo>
															<m:mrow>
