								<m:mtd columnalign="left">
									<m:mrow>
										<m:mfrac>
											<m:mrow>
												<m:mo>-</m:mo><m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>-</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2</m:mn>
											</m:mrow>
											<m:mrow>
												<m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>15</m:mn>
											</m:mrow>
										</m:mfrac>
										<m:mo>·</m:mo><m:mfrac>
											<m:mrow>
												<m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>20</m:mn>
											</m:mrow>
											<m:mrow>
												<m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi>
											</m:mrow>
										</m:mfrac>
										<m:mo>.</m:mo>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mtext>Factor –1 from the first numerator</m:mtext><m:mo>.</m:mo>
									</m:mrow>
								</m:mtd>
							</m:mtr>
							<m:mtr columnalign="left">
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mfrac>
											<m:mrow>
												<m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>
													<m:mrow>
														<m:msup>
															<m:mi>x</m:mi>
															<m:mn>2</m:mn>
														</m:msup>
														<m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
													</m:mrow>
													<m:mo>)</m:mo></m:mrow>
											</m:mrow>
											<m:mrow>
												<m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>15</m:mn>
											</m:mrow>
										</m:mfrac>
										<m:mo>·</m:mo><m:mfrac>
											<m:mrow>
												<m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>20</m:mn>
											</m:mrow>
											<m:mrow>
												<m:msup>
													<m:mi>x</m:mi>
													<m:mn>2</m:mn>
												</m:msup>
												<m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi>
											</m:mrow>
										</m:mfrac>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mtext>Factor</m:mtext><m:mo>.</m:mo>
									</m:mrow>
								</m:mtd>
							</m:mtr>
							<m:mtr columnalign="left">
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mfrac>
											<m:mrow>
												<m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>
													<m:mrow>
														<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
													</m:mrow>
													<m:mo>)</m:mo></m:mrow><m:menclose notation="updiagonalstrike">
													<m:mrow>
														<m:mrow><m:mo>(</m:mo>
															<m:mrow>
																<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
															</m:mrow>
															<m:mo>)</m:mo></m:mrow>
													</m:mrow>
												</m:menclose>
											</m:mrow>
											<m:mrow>
												<m:mrow><m:mo>(</m:mo>
