														<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
													</m:mrow>
													<m:mo>)</m:mo></m:mrow><m:menclose notation="updiagonalstrike">
													<m:mrow><m:mo>(</m:mo>
														<m:mrow>
															<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
														</m:mrow>
														<m:mo>)</m:mo></m:mrow></m:menclose>
											</m:mrow>
										</m:mfrac>
										<m:mo>·</m:mo><m:mfrac>
											<m:mrow>
												<m:mn>4</m:mn><m:menclose notation="updiagonalstrike"><m:mrow><m:mo>(</m:mo>
														<m:mrow>
															<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
														</m:mrow>
														<m:mo>)</m:mo></m:mrow>
												</m:menclose>
											</m:mrow>
											<m:mrow>
