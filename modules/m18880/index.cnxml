    <m:mn>7</m:mn>
    <m:mn>2</m:mn>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id22162285"><problem id="fs-id22162286"><para id="fs-id22162287"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>3</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id3957408"><problem id="fs-id3957409"><para id="fs-id3957410"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>7</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>7</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
