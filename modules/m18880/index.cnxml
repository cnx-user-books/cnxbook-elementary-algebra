    <m:mn>6</m:mn>
    <m:mn>5</m:mn>
   </m:mfrac>
   <m:mtext> </m:mtext><m:mtext>or</m:mtext><m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>3</m:mn>
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id8664978"><problem id="fs-id8664979"><para id="fs-id8664980"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>5</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>2</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id20118410"><problem id="fs-id20118411"><para id="fs-id20118412"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>6</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>11</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem><solution id="zip-id32762166"><para id="zip-id32762167"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>m</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
    <m:mn>5</m:mn>
    <m:mn>6</m:mn>
   </m:mfrac>
   <m:mtext> </m:mtext><m:mtext>or</m:mtext><m:mtext> </m:mtext><m:mi>m</m:mi><m:mo>=</m:mo><m:mfrac>
    <m:mn>6</m:mn>
    <m:mrow>
     <m:mn>11</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></solution></exercise><exercise id="fs-id19734869"><problem id="fs-id19734870"><para id="fs-id19734871"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>2</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>3</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem></exercise><exercise id="fs-id20128077"><problem id="fs-id20128078"><para id="fs-id20128079"><m:math> <m:semantics>  <m:mrow>   <m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>    <m:mrow>     <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>    </m:mrow>   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0</m:mn>  </m:mrow>   </m:semantics></m:math></para>
</problem><solution id="zip-id32474149"><para id="zip-id32474150"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>5</m:mn>
