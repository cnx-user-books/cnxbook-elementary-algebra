											<m:mtext>To</m:mtext><m:mtext> </m:mtext><m:mtext>use</m:mtext><m:mtext> </m:mtext><m:mtext>this</m:mtext><m:mtext> </m:mtext><m:mtext>form,</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext><m:mtext> </m:mtext>
										</m:mtd>
									</m:mtr>
									<m:mtr>
										<m:mtd>
											<m:mtext>slope</m:mtext><m:mtext> </m:mtext><m:mtext>and</m:mtext><m:mtext> </m:mtext><m:mi>y</m:mi><m:mtext>-intercept</m:mtext>
										</m:mtd>
									</m:mtr>
									<m:mtr>
										<m:mtd>
											<m:mtext>are</m:mtext><m:mtext> </m:mtext><m:mtext>needed</m:mtext><m:mtext>.</m:mtext>
										</m:mtd>
									</m:mtr>
								</m:mtable>
							</m:mtd>
							<m:mtd>
								<m:mtable columnalign="left">
									<m:mtr>
										<m:mtd>
											<m:mtext>To</m:mtext><m:mtext> </m:mtext><m:mtext>use</m:mtext><m:mtext> </m:mtext><m:mtext>this</m:mtext><m:mtext> </m:mtext><m:mtext>form,</m:mtext><m:mtext> </m:mtext><m:mtext>the</m:mtext>
										</m:mtd>
									</m:mtr>
									<m:mtr>
										<m:mtd>
											<m:mtext>slope</m:mtext><m:mtext> </m:mtext><m:mtext>and</m:mtext><m:mtext> </m:mtext><m:mtext>one</m:mtext><m:mtext> </m:mtext><m:mtext>point,</m:mtext>
										</m:mtd>
									</m:mtr>
									<m:mtr>
										<m:mtd>
											<m:mtext>or</m:mtext><m:mtext> </m:mtext><m:mtext>two</m:mtext><m:mtext> </m:mtext><m:mtext>points,</m:mtext><m:mtext> </m:mtext><m:mtext>are</m:mtext><m:mtext> </m:mtext><m:mtext>needed</m:mtext><m:mtext>.</m:mtext>
