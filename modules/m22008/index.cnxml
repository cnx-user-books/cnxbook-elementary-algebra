											<m:mtext>point-slope</m:mtext><m:mtext> </m:mtext><m:mtext>from</m:mtext>
										</m:mrow>
										<m:mo stretchy="true">¯</m:mo>
									</m:munder>
								</m:mrow>
							</m:mtd>
						</m:mtr>
						<m:mtr>
							<m:mtd>
								<m:mrow>
									<m:mi>a</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mi>y</m:mi><m:mo>=</m:mo><m:mi>c</m:mi>
								</m:mrow>
							</m:mtd>
