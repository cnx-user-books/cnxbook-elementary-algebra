	</m:math>
</para></example>
<example id="fs-id15041975"><para id="fs-id15796251"><m:math display="block">
		<m:semantics>
			<m:mrow>
				<m:mtable columnalign="left">
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mo>-</m:mo><m:mfrac>
									<m:mn>3</m:mn>
									<m:mrow>
										<m:mn>7</m:mn><m:mo>-</m:mo><m:mi>x</m:mi>
									</m:mrow>
								</m:mfrac>
								<m:mo>.</m:mo>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mtext>Rewrite this</m:mtext><m:mtext>.</m:mtext>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mfrac>
									<m:mrow>
										<m:mo>-</m:mo><m:mn>3</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mn>7</m:mn><m:mo>-</m:mo><m:mi>x</m:mi>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mtext>Factor out</m:mtext><m:mo>-</m:mo><m:mtext>1 from the deno</m:mtext><m:mi>min</m:mi><m:mtext>ator</m:mtext><m:mtext>.</m:mtext>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mfrac>
									<m:mrow>
										<m:mo>-</m:mo><m:mn>3</m:mn>
									</m:mrow>
									<m:mrow>
										<m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>
											<m:mrow>
												<m:mo>-</m:mo><m:mn>7</m:mn><m:mo>+</m:mo><m:mi>x</m:mi>
											</m:mrow>
											<m:mo>)</m:mo></m:mrow>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mtext>A negative divided by a negative is positive</m:mtext><m:mo>.</m:mo>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mfrac>
									<m:mn>3</m:mn>
									<m:mrow>
										<m:mo>-</m:mo><m:mn>7</m:mn><m:mo>+</m:mo><m:mi>x</m:mi>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mtext>Rewrite</m:mtext><m:mo>.</m:mo>
							</m:mrow>
						</m:mtd>
					</m:mtr>
					<m:mtr columnalign="left">
						<m:mtd columnalign="left">
							<m:mrow>
								<m:mfrac>
									<m:mn>3</m:mn>
									<m:mrow>
										<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>7</m:mn>
									</m:mrow>
								</m:mfrac>
							</m:mrow>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
						<m:mtd columnalign="left">
							<m:mrow/>
						</m:mtd>
					</m:mtr>
				</m:mtable>
			</m:mrow></m:semantics>
	</m:math>
	<newline/><newline/>This expression seems less cumbersome than does the original (fewer minus signs).</para></example>

