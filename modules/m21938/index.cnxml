									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mn>6</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mo>+</m:mo><m:mn>6</m:mn><m:mo>+</m:mo><m:mn>2</m:mn>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow><m:mo>=</m:mo>
									</m:mrow>
								</m:mtd>
