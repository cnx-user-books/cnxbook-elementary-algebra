      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>The LCD is</m:mtext><m:mi> </m:mi><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>.</m:mo><m:mi> </m:mi><m:mtext>Multiply each term by</m:mtext><m:mi> </m:mi><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>.</m:mo>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>6</m:mn><m:mi>x</m:mi><m:mtext> </m:mtext><m:mi>·</m:mi><m:mtext> </m:mtext><m:mfrac>
        <m:mn>2</m:mn>
        <m:mn>3</m:mn>
       </m:mfrac>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mtext> </m:mtext><m:mi>·</m:mi><m:mtext> </m:mtext><m:mfrac>
        <m:mn>1</m:mn>
        <m:mi>x</m:mi>
       </m:mfrac>
       
