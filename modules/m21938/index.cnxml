									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow><m:mtext>Is this correct?</m:mtext></m:mrow>
								</m:mtd>
							</m:mtr>
							<m:mtr>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="right">
									<m:mrow>
										<m:mn>16</m:mn>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mo>=</m:mo>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mn>16</m:mn>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow><m:mtext>Yes, this is correct.</m:mtext></m:mrow>
								</m:mtd>
							</m:mtr>
							<m:mtr>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="right">
									<m:mrow>
										<m:mtext>Since </m:mtext><m:mi>x</m:mi>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mo>=</m:mo>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow>
										<m:mn>6</m:mn><m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
											<m:mi>x</m:mi>
											<m:mn>3</m:mn>
										</m:mfrac>
										<m:mo>=</m:mo><m:mfrac>
											<m:mn>6</m:mn>
											<m:mn>3</m:mn>
										</m:mfrac>
										<m:mo>=</m:mo><m:mn>2</m:mn>
									</m:mrow>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
								<m:mtd columnalign="left">
									<m:mrow/>
								</m:mtd>
							</m:mtr>
						</m:mtable>
					</m:mtd>
				</m:mtr>
				<m:mtr>
					<m:mtd columnalign="left">
						<m:mtext>Step 5</m:mtext><m:mo>:</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext>The length = 6 cm and the width = 2 cm</m:mtext><m:mtext>.</m:mtext>
					</m:mtd>
				</m:mtr>
			</m:mtable>
		</m:semantics>
	</m:math>
</para></example>
