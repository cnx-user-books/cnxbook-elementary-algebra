     </m:mfrac>
     
    </m:mrow>
    <m:mrow>
     <m:mfrac>
      <m:mi>x</m:mi>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi>
      </m:mrow>
     </m:mfrac>
     <m:mo>+</m:mo><m:mfrac>
      <m:mi>y</m:mi>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mi>y</m:mi>
      </m:mrow>
     </m:mfrac>
     
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>
  
 </m:semantics>
</m:math>
</para></problem></exercise>
<exercise id="fs-id15550379"><problem id="fs-id15550380"><para id="fs-id15550382"><m:math>
