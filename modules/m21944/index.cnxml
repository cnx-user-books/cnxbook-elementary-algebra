           </m:mtd>
           <m:mtd columnalign="left">
            <m:mrow>
             <m:mfrac>
              <m:mrow>
               <m:msup>
                <m:mi>x</m:mi>
                <m:mn>2</m:mn>
               </m:msup>
               <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
              </m:mrow>
              <m:mrow>
               <m:msup>
                <m:mi>x</m:mi>
                <m:mn>2</m:mn>
               </m:msup>
               <m:mo>+</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
              </m:mrow>
             </m:mfrac>
             
            </m:mrow>
           </m:mtd>
          </m:mtr>
          
         </m:mtable>
        </m:mrow>
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext>Step</m:mtext><m:mtext> </m:mtext><m:mn>3</m:mn><m:mo>:</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext>Reduce</m:mtext><m:mtext>.</m:mtext>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtable columnalign="left">
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
          </m:mrow>
          <m:mrow>
           <m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>+</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext>Thus,</m:mtext>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mfrac>
      <m:mrow>
       <m:mn>1</m:mn><m:mo>−</m:mo><m:mfrac>
        <m:mn>5</m:mn>
        <m:mi>x</m:mi>
       </m:mfrac>
       <m:mo>−</m:mo><m:mfrac>
        <m:mn>6</m:mn>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
      <m:mrow>
       <m:mn>1</m:mn><m:mo>+</m:mo><m:mfrac>
        <m:mn>6</m:mn>
        <m:mi>x</m:mi>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mn>5</m:mn>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:mfrac>
     <m:mo>=</m:mo><m:mfrac>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn>
      </m:mrow>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
      </m:mrow>
     </m:mfrac>
     
    </m:mtd>
   </m:mtr>
  </m:mtable>
  
  
 </m:semantics>
</m:math>


</para></example>
</section>

<section id="fs-id15556798">
 <title>Practice Set B</title>
<para id="fs-id15556804">The following problems are the same problems as the problems in Practice Set A. Simplify these expressions using the LCD-multiply-divide method. Compare the answers to the answers produced in Practice Set A. </para>
