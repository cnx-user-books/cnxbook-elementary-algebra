           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mo>+</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>16</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         </m:mfrac>
         <m:mo>.</m:mo>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mtable columnalign="left">
         <m:mtr>
          <m:mtd>
           <m:mtext>4 is a nondomain value</m:mtext><m:mtext>. Exclude it from consideration</m:mtext><m:mtext>.</m:mtext><m:mtext> </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>The LCD is </m:mtext><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4.</m:mn><m:mtext> </m:mtext><m:mtext>Multiply every term by</m:mtext><m:mtext> </m:mtext><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4.</m:mn>
          </m:mtd>
         </m:mtr>
        </m:mtable>
        
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:mi>a</m:mi>
          </m:mrow>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
          <m:mrow>
           <m:mn>16</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mrow>
             <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
           </m:menclose>
           
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:mi>a</m:mi>
          </m:mrow>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mrow>
             <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
           </m:menclose>
           
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mrow>
             <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
           </m:menclose>
           
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
          <m:mrow>
           <m:mn>16</m:mn>
          </m:mrow>
          <m:mrow>
           <m:menclose notation="updiagonalstrike">
            <m:mrow>
             <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
           </m:menclose>
           
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mn>4</m:mn><m:mi>a</m:mi>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
          </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>16</m:mn>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mtext>Solve this nonfractional equation.</m:mtext>
        </m:mrow>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mn>4</m:mn><m:mi>a</m:mi>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>8</m:mn><m:mo>+</m:mo><m:mn>16</m:mn>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mn>4</m:mn><m:mi>a</m:mi>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow>
         <m:mn>2</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mrow>
         <m:mn>2</m:mn><m:mi>a</m:mi>
        </m:mrow>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mn>8</m:mn>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr columnalign="left">
       <m:mtd columnalign="right">
        <m:mi>a</m:mi>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mn>4</m:mn>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
       <m:mtd columnalign="left">
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext>This value, </m:mtext><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mtext>has been excluded from consideration</m:mtext><m:mtext>. It is not to be considered as a solution</m:mtext><m:mtext>. It is extraneous</m:mtext><m:mtext>. </m:mtext>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext>As there are no other potential solutions to consider, we conclude that this equation has </m:mtext><m:mi>n</m:mi><m:mi>o</m:mi><m:mtext> </m:mtext><m:mi>s</m:mi><m:mi>o</m:mi><m:mi>l</m:mi><m:mi>u</m:mi><m:mi>t</m:mi><m:mi>i</m:mi><m:mi>o</m:mi><m:mi>n</m:mi><m:mtext>.</m:mtext>
    </m:mtd>
   </m:mtr>
  </m:mtable>
  

 </m:semantics>
</m:math>


</para></example>
</section>
<section id="fs-id5973030">
      <title>Practice Set A</title>
	  <para id="fs-id5973035">Solve the following rational equations.</para>
<exercise id="fs-id5973038">
<problem id="fs-id5973040"><para id="fs-id5973041"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>2</m:mn><m:mi>x</m:mi>
    </m:mrow>
    <m:mn>5</m:mn>
   </m:mfrac>
   <m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>14</m:mn>
    </m:mrow>
    <m:mn>6</m:mn>
   </m:mfrac>
   
  </m:mrow>

 </m:semantics>
</m:math>
</para></problem>
<solution id="fs-id5973087"><para id="fs-id5973088"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>x</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>10</m:mn>
  </m:mrow>

 </m:semantics>
</m:math>
</para></solution>
</exercise>
<exercise id="fs-id5973111">
<problem id="fs-id5973113"><para id="fs-id5973114"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>3</m:mn><m:mi>a</m:mi>
    </m:mrow>
    <m:mrow>
     <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>3</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>

 </m:semantics>
</m:math>
</para></problem>
<solution id="fs-id5973177"><para id="fs-id5973178"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>a</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
  </m:mrow>

 </m:semantics>
</m:math>
</para></solution>
</exercise>
<exercise id="fs-id5973200">
<problem id="fs-id5973202"><para id="fs-id5973204"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mn>3</m:mn>
    <m:mrow>
     <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>+</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mfrac>
    <m:mi>y</m:mi>
    <m:mrow>
     <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
    </m:mrow>
   </m:mfrac>
   
  </m:mrow>

 </m:semantics>
</m:math>
</para></problem>
<solution id="fs-id5973256"><para id="fs-id5973257"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mi>y</m:mi><m:mo>=</m:mo><m:mn>3</m:mn>
  </m:mrow>

 </m:semantics>
</m:math> is extraneous, so no solution.
</para></solution>
</exercise>
</section>
<section id="fs-id5973278">
      <title>Sample Set B</title>	  
<para id="fs-id5973284">Solve the following rational equations.</para>
<example id="fs-id5973287"><para id="fs-id5973288"><m:math display="block">
 <m:semantics>
  <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mtable>
      <m:mtr>
       <m:mtd>
        <m:mrow>
         <m:mfrac>
          <m:mn>3</m:mn>
          <m:mi>x</m:mi>
         </m:mfrac>
         <m:mo>+</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:mi>x</m:mi>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd>
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd>
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>+</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
          </m:mrow>
          <m:mrow>
           <m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mi>x</m:mi>
          </m:mrow>
         </m:mfrac>
         <m:mo>.</m:mo>
        </m:mrow>
       </m:mtd>
       <m:mtd>
        <m:mrow/>
       </m:mtd>
       <m:mtd>
        <m:mtable columnalign="left">
         <m:mtr>
          <m:mtd>
           <m:mtext>Factor all denominators to find any </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>excluded values and the LCD</m:mtext><m:mtext>.</m:mtext>
          </m:mtd>
         </m:mtr>
        </m:mtable>
        
       </m:mtd>
      </m:mtr>
      <m:mtr>
       <m:mtd>
        <m:mrow>
         <m:mfrac>
          <m:mn>3</m:mn>
          <m:mi>x</m:mi>
         </m:mfrac>
         <m:mo>+</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:mi>x</m:mi>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd>
        <m:mo>=</m:mo>
       </m:mtd>
       <m:mtd>
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>+</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
          </m:mrow>
          <m:mrow>
           <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow>
          </m:mrow>
         </m:mfrac>
         
        </m:mrow>
       </m:mtd>
       <m:mtd>
        <m:mrow/>
       </m:mtd>
       <m:mtd>
        <m:mtable columnalign="left">
         <m:mtr>
          <m:mtd>
           <m:mtext>Nondomain values are 0 and 1</m:mtext><m:mtext>. </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>Exclude them from consideration</m:mtext><m:mtext>.</m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>The LCD is </m:mtext><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo><m:mtext> </m:mtext><m:mtext>Multiply each </m:mtext>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mtext>term by </m:mtext><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
            </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mtext>and simplify</m:mtext><m:mtext>.</m:mtext>
          </m:mtd>
         </m:mtr>
        </m:mtable>
        
       </m:mtd>
      </m:mtr>
      
     </m:mtable>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:menclose notation="updiagonalstrike">
      <m:mi>x</m:mi>
     </m:menclose>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
      <m:mn>3</m:mn>
      <m:mrow>
       <m:menclose notation="updiagonalstrike">
        <m:mi>x</m:mi>
       </m:menclose>
       
      </m:mrow>
     </m:mfrac>
     <m:mo>+</m:mo><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:menclose notation="updiagonalstrike">
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:menclose>
       
      </m:mrow>
     <m:mo>)</m:mo></m:mrow><m:mtext> </m:mtext><m:mo>·</m:mo><m:mtext> </m:mtext><m:mfrac>
      <m:mrow>
       <m:mn>4</m:mn><m:mi>x</m:mi>
      </m:mrow>
      <m:mrow>
       <m:menclose notation="updiagonalstrike">
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:menclose>
       
