											</m:msup>
											<m:msup>
												<m:mi>y</m:mi>
												<m:mrow>
													<m:mo>−</m:mo><m:mn>4</m:mn>
												</m:mrow>
											</m:msup>
										</m:mrow>
										<m:mo>)</m:mo></m:mrow>
								</m:mrow>
								<m:mrow>
