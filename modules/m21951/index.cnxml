    </m:mrow>
   </m:mfrac>
   <m:mo>−</m:mo><m:mfrac>
    <m:mn>1</m:mn>
    <m:mrow>
     <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>2</m:mn><m:mi>x</m:mi>
    </m:mrow>
    <m:mrow>
     <m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>.</m:mo>
  </m:mrow>

 </m:semantics>
</m:math>
</para></problem>
<solution id="fs-id5973760"><para id="fs-id5973762">This equation has no solution. <m:math>
 <m:semantics>
