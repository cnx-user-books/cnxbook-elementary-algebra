					<m:mn>3</m:mn>
				</m:msup>
				<m:mo>⋅</m:mo><m:mn>6</m:mn><m:msup>
					<m:mi>y</m:mi>
					<m:mn>2</m:mn>
				</m:msup>
				<m:mo>=</m:mo><m:mtable frame="solid"><m:mtr><m:mtd><m:mrow>
						<m:mn>4</m:mn><m:mo>⋅</m:mo><m:mn>6</m:mn><m:mo>⋅</m:mo><m:msup>
							<m:mi>y</m:mi>
							<m:mrow>
								<m:mn>3</m:mn><m:mo>+</m:mo><m:mn>2</m:mn>
							</m:mrow>
						</m:msup>
					</m:mrow></m:mtd></m:mtr></m:mtable>
				<m:mo>=</m:mo><m:mn>24</m:mn><m:msup>
					<m:mi>y</m:mi>
					<m:mn>5</m:mn>
				</m:msup>
			</m:mrow>
			<m:annotation encoding="MathType-MTEF">
			</m:annotation>
		</m:semantics>
	</m:math>
</para>
</example><example id="eip-90"><para id="eip-241"><m:math>
		<m:semantics>
			<m:mrow>
