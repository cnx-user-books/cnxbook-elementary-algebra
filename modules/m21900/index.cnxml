</exercise><exercise id="eip-656"><problem id="eip-283">
		<para id="eip-100">
    <m:math>
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mn>26</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:msup>
      <m:mi>y</m:mi>
      <m:mn>6</m:mn>
     </m:msup>
     <m:msup>
      <m:mi>z</m:mi>
