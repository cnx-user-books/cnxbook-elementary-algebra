 </m:semantics>
</m:math>
</para></solution>	</exercise><exercise id="eip-557"><problem id="eip-569">			<para id="eip-76">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>6</m:mn><m:msup>								<m:mi>a</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:mo>+</m:mo><m:mn>7</m:mn><m:mi>a</m:mi><m:mi>b</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:msup>								<m:mi>b</m:mi>								<m:mn>2</m:mn>							</m:msup>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
