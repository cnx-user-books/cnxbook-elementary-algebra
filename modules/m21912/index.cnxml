							<m:msup>
								<m:mrow>
									<m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>n</m:mi><m:mo stretchy="false">)</m:mo>
