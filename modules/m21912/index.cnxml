</problem>	</exercise><exercise id="eip-390"><problem id="eip-236">			<para id="eip-3">				<m:math>					<m:semantics>						<m:mrow>							<m:mn>14</m:mn><m:msup>								<m:mi>a</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:msup>								<m:mi>z</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:mo>−</m:mo><m:mn>40</m:mn><m:msup>								<m:mi>a</m:mi>								<m:mn>3</m:mn>							</m:msup>							<m:msup>								<m:mi>z</m:mi>								<m:mn>2</m:mn>							</m:msup>							<m:mo>−</m:mo><m:mn>46</m:mn><m:msup>								<m:mi>a</m:mi>								<m:mn>4</m:mn>							</m:msup>							<m:msup>								<m:mi>z</m:mi>								<m:mn>2</m:mn>							</m:msup>						</m:mrow>						<m:annotation encoding="MathType-MTEF">						</m:annotation>					</m:semantics>				</m:math>			</para>		
