						<m:mtable columnalign="left">
							<m:mtr>
								<m:mtd>
									<m:mtext>width</m:mtext><m:mo>=</m:mo><m:mfrac>
										<m:mrow>
											<m:mo>−</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:msqrt>
												<m:mrow>
													<m:mn>1793</m:mn>
												</m:mrow>
											</m:msqrt>
										</m:mrow>
										<m:mn>4</m:mn>
									</m:mfrac>
									<m:mo>;</m:mo>
