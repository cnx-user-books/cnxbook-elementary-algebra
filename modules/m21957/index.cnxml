											<m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
										</m:mrow>
										<m:mo>)</m:mo></m:mrow>
								</m:mrow>
								<m:mrow>
									<m:mo>−</m:mo><m:mn>7</m:mn>
