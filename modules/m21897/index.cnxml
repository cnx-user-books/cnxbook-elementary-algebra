											</m:mrow>
											<m:mn>3</m:mn>
										</m:msup>
										<m:mo>=</m:mo><m:msup>
											<m:mi>a</m:mi>
											<m:mrow>
												<m:mn>3</m:mn><m:mtext> </m:mtext><m:mo>⋅</m:mo><m:mtext> </m:mtext><m:mn>2</m:mn>
