							</m:mrow>
						</m:mtd>
						<m:mtd>
							<m:mrow>
								<m:mtext>The</m:mtext><m:mtext> </m:mtext><m:mtext>box</m:mtext><m:mtext> </m:mtext><m:mtext>represents</m:mtext><m:mtext> </m:mtext><m:mtext>a</m:mtext><m:mtext> </m:mtext><m:mtext>step</m:mtext><m:mtext> </m:mtext><m:mtext>done</m:mtext><m:mtext> </m:mtext><m:mtext>mentally.</m:mtext>
							</m:mrow>
						</m:mtd>
					</m:mtr>
				</m:mtable>
			</m:mrow>
			<m:annotation encoding="MathType-MTEF">
