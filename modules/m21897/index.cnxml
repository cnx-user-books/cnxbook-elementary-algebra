											<m:mrow>
												<m:mn>3</m:mn><m:mtext> </m:mtext><m:mo>⋅</m:mo><m:mtext> </m:mtext><m:mn>4</m:mn>
											</m:mrow>
										</m:msup>
									</m:mrow></m:mtd></m:mtr></m:mtable>
								<m:mtext> </m:mtext><m:msup>
